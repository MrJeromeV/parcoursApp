<?xml version='1.0' encoding='utf-8'?>
<widget id="org.ummagumma.parcours" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>ParcoursApp</name>
    <description>
        Une application d'aide à l'élaboration de parcours de tir à l'arc.
    </description>
    <author email="jeromev@ummagumma.org" href="http://ummagumma.org">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="res/icon/icon-36-ldpi.png" />
        <icon density="mdpi" src="res/icon/icon-48-mdpi.png" />
        <icon density="hdpi" src="res/icon/icon-72-hdpi.png" />
        <icon density="xhdpi" src="res/icon/icon-96-xhdpi.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^6.4.0" />
    <engine name="browser" spec="^5.0.1" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-device-orientation" spec="^1.0.7" />
    <plugin name="cordova-plugin-geolocation" spec="^2.4.3" />
    <plugin name="cordova-plugin-file" spec="^4.3.3" />
    <plugin name="cordova-plugin-email" spec="^1.2.6" />
    <plugin name="cordova-plugin-dialogs" spec="^1.3.4" />
</widget>
